    local Other = Window:NewTab("Player")
    local OtherSection = Other:NewSection("Player")
    
    OtherSection:NewSlider("Walkspeed", "go zoom", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)

    OtherSection:NewSlider("Jump Power", "Jump height", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
        game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = s
