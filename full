local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("MHM Script", "Serpent")

     -- Function

    function autorebirthlegit()
        while _G.autorebirthlegit == true do
            game:GetService("ReplicatedStorage").Reb_irth:InvokeServer()
            wait(math.random(1.623,2.478))
        end
    end

    function autorebirth()
        while _G.autorebirth == true do
            game:GetService("ReplicatedStorage").Reb_irth:InvokeServer()
            wait(0.03)
        end
    end

    function autolayout1()
        while _G.autolayout1 == true do
            game:GetService("HttpService"):JSONDecode(game.Players.LocalPlayer.Layouts.Layout1.Value)
            local ohString1 = "Load"
            local ohString2 = "Layout1"
            game:GetService("ReplicatedStorage").Layouts:InvokeServer(ohString1, ohString2)
            wait(math.random(0.13,0.27))
        end
    end

    function autolayout2()
        while _G.autolayout2 == true do
            game:GetService("HttpService"):JSONDecode(game.Players.LocalPlayer.Layouts.Layout2.Value)
            local ohString1 = "Load"
            local ohString2 = "Layout2"
            game:GetService("ReplicatedStorage").Layouts:InvokeServer(ohString1, ohString2)
            wait(math.random(0.13,0.27))
        end
    end

    function autolayout3()
        while _G.autolayout3 == true do
            game:GetService("HttpService"):JSONDecode(game.Players.LocalPlayer.Layouts.Layout3.Value)
            local ohString1 = "Load"
            local ohString2 = "Layout3"
            game:GetService("ReplicatedStorage").Layouts:InvokeServer(ohString1, ohString2)
            wait(math.random(0.13,0.27))
        end
    end


    -- MAIN
    local Main = Window:NewTab("Automation")
    local MainSection = Main:NewSection("Automation")

    MainSection:NewToggle("Rebirth Legit", "ToggleInfo", function(state)
        if state then
            print("Toggle On")
            _G.autorebirthlegit = state
            autorebirthlegit()
        else
            print("Toggle Off")
            _G.autorebirthlegit = state
            autorebirthlegit()
        end
    end)

    MainSection:NewToggle("Rebirth Blatant", "ToggleInfo", function(state)
        if state then
            print("Toggle On")
            _G.autorebirth = state
            autorebirth()
        else
            print("Toggle Off")
            _G.autorebirth = state
            autorebirth()
        end
    end)

    MainSection:NewToggle("Layout 1", "ToggleInfo", function(state)
        if state then
            print("Toggle On")
            _G.autolayout1 = state
            autolayout1()
        else
            print("Toggle Off")
            _G.autolayout1 = state
            autolayout1()
        end
    end)

    MainSection:NewToggle("Layout 2", "ToggleInfo", function(state)
        if state then
            print("Toggle On")
            _G.autolayout2 = state
            autolayout2()
        else
            print("Toggle Off")
            _G.autolayout2 = state
            autolayout2()
        end
    end)

    MainSection:NewToggle("Layout 3", "ToggleInfo", function(state)
        if state then
            print("Toggle On")
            _G.autolayout3 = state
            autolayout3()
        else
            print("Toggle Off")
            _G.autolayout3 = state
            autolayout3()
        end
    end)

